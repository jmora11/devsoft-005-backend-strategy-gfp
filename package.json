{
  "name": "devsoft-005-backend-strategy-gfp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf .nyc_output coverage build .scannerwork performance/*.clinic-doctor*",
    "build": "tsc",
    "start": "npm run build && DEBUG=gfp:* node ./build/index.js",
    "dev": "nodemon --watch src -e ts,js --exec npm start",
    "test:file": "export $(cat test/.env-test | xargs) && mocha --timeout 10000 --require ts-node/register",
    "test:file:example": "npm run test:file -- 'test/example.spec.ts' -g 'Test Class Name'",
    "test:only": "npm run test:file -- 'test/**/*.spec.ts'",
    "test": "npm run lint && npm run test:only",
    "coverage": "export $(cat test/.env-test | xargs) && nyc _mocha --require ts-node/register 'test/**/*.spec.ts'",
    "coverage:report": "nyc report --reporter=lcov --reporter=text",
    "coverage:web": "npm run coverage && http-server coverage/",
    "sonar": "npm run coverage && npm run coverage:report && gulp sonar",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "debug": "DEBUG=* node ./build/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.2",
    "express-actuator": "^1.8.0",
    "express-openapi-validate": "^0.6.1",
    "faker": "^6.6.6",
    "http-server": "^14.0.0",
    "mocha": "^9.1.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.6.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.13",
    "@types/express-actuator": "^1.8.0",
    "@types/faker": "^5.5.9",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/sinon": "^10.0.6",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "nock": "^13.2.1",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "sequelize-mock": "^0.10.2",
    "sinon": "^12.0.1",
    "tslint": "^5.17.0"
  }
}
